<div class=row>
  <h1>New cart</h1>
</div>

<%= form_for @cart, class: "form-inline" do |f| %>
  <div class="form-group">
    <%= f.label :cart_name %>
    <%= f.text_field :name, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.submit "Add", class: "btn btn-primary" %>
  </div>
<% end %>